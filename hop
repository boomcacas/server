local a={}local b=""local c=os.date("!*t").hour;local d=false;local e=game:GetService("TeleportService")local f=game:GetService("HttpService")local g=pcall(function()a=f:JSONDecode(readfile("server-hop-temp.json"))end)if not g then table.insert(a,c)pcall(function()writefile("server-hop-temp.json",f:JSONEncode(a))end)end;local function h(i)local j;if b==""then j=f:JSONDecode(game:HttpGet('https://games.roblox.com/v1/games/'..i..'/servers/Public?sortOrder=Asc&limit=100'))else j=f:JSONDecode(game:HttpGet('https://games.roblox.com/v1/games/'..i..'/servers/Public?sortOrder=Asc&limit=100&cursor='..b))end;local k=""if j.nextPageCursor and j.nextPageCursor~="null"and j.nextPageCursor~=nil then b=j.nextPageCursor end;local l=0;for m,n in pairs(j.data)do local o=true;k=tostring(n.id)if tonumber(n.maxPlayers)>tonumber(n.playing)then for p,q in pairs(a)do if l~=0 then if k==tostring(q)then o=false end else if tonumber(c)~=tonumber(q)then local r=pcall(function()delfile("server-hop-temp.json")a={}table.insert(a,c)end)end end;l=l+1 end;if o==true then table.insert(a,k)wait()pcall(function()writefile("server-hop-temp.json",f:JSONEncode(a))wait()e:TeleportToPlaceInstance(i,k,game.Players.LocalPlayer)end)wait(4)end end end end;local s={}function s:Teleport(i)while wait()do pcall(function()h(i)if b~=""then h(i)end end)end end;return s
